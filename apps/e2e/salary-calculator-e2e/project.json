{
  "name": "salary-calculator-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/e2e/salary-calculator-e2e/src",
  "tags": [],
  "implicitDependencies": ["salary-calculator"],
  "projectType": "application",
  "targets": {
    "e2e-headed": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/e2e/salary-calculator-e2e/playwright.config.ts",
        "ui": true
      },
      "configurations": {
        "production": {
          "devServerTarget": "salary-calculator:serve:production"
        }
      }
    },
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/e2e/salary-calculator-e2e/playwright.config.ts"
      }
    },

    "e2e-ui": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx playwright test --ui",
        "cwd": "apps/e2e/salary-calculator-e2e"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
