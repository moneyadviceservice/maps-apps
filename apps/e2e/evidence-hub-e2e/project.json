{
  "name": "evidence-hub-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/e2e/evidence-hub-e2e/src",
  "tags": [],
  "implicitDependencies": ["evidence-hub"],
  "targets": {
    "e2e-headed": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/e2e/evidence-hub-e2e/playwright.config.ts",
        "ui": true
      },
      "configurations": {
        "production": {
          "devServerTarget": "evidence-hub:serve:production"
        }
      }
    },
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/e2e/evidence-hub-e2e/playwright.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
